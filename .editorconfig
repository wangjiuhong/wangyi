root = true
[*] #所有文件适用
charset = utf-8  #设置字符集utf-8
indent_style = space    #缩进风格，tab或space
indent_size = 2          #缩进大小
end_of_line = lf        #换行符，lf或crlf
insert_final_newline = true #始终在文件末尾添加一个新行
trim_trailing_whitespace = true #去除行尾的任意空白字符

[*.md] #Markdown文件适用
max_line_length = off
trim_trailing_whitespace = false